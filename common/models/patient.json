{
  "name": "patient",
  "plural": "patients",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "title": {
      "type": "string",
      "default": "Mr"
    },
    "firstName": {
      "type": "string",
      "required": true
    },
    "middleName": {
      "type": "string"
    },
    "lastName": {
      "type": "string",
      "required": true
    },
    "dateOfBirth": {
      "type": "date",
      "required": true
    },
    "medicareNumber": {
      "type": "number",
      "required": true
    },
    "sex": {
      "type": "string",
      "required": true
    },
    "maritalStatus": {
      "type": "string"
    },
    "height": {
      "type": "number"
    }
  },
  "validations": [],
  "relations": {
    "contacts": {
      "type": "hasOne",
      "model": "contact",
      "foreignKey": "patientId"
    },
    "insurances": {
      "type": "hasOne",
      "model": "insurance",
      "foreignKey": "patientId"
    },
    "sensorData": {
      "type": "hasMany",
      "model": "sensorData",
      "foreignKey": "patientId"
    },
    "carePlans": {
      "type": "hasOne",
      "model": "carePlan",
      "foreignKey": "patientId"
    }
  },
  "acls": [],
  "methods": {}
}
